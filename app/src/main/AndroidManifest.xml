<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.aaa.TicTacToeXO">

    <uses-permission android:name="android.permission.INTERNET" />

    <supports-screens
        android:xlargeScreens="true"
        android:largeScreens="true"/>

    <application
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:allowBackup="true">
        <activity android:name="com.aaa.TicTacToeXO.playComputerActivity" />
        <activity android:name="com.aaa.TicTacToeXO.formActivity" />
        <activity android:name="com.aaa.TicTacToeXO.MainActivity" />
        <activity android:name=".MainPage"/>
        <activity android:name="com.aaa.TicTacToeXO.loadPage">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>